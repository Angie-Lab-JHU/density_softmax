      "source": [
        "# get imagenet_id: wordnet_id mapping. e.g., '0': 'n01440764'\n",
        "index_wdid = {}\n",
        "with tf.io.gfile.GFile(imagenet_label_to_wordnet_file, mode='r') as f:\n",
